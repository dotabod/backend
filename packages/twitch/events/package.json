  "author": "Geczy",
  "main": "src/index.ts",
  "exports": "./dist/index.js",
  "type": "module",
  "packageManager": "yarn@3.7.0",
  "private": true,
  "scripts": {
    "docker:production": "node --trace-warnings ./dist/index.js",
