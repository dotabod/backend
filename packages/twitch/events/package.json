    "@twurple/eventsub-http": "6.0.9",
    "express": "^4.18.2",
    "i18next": "^23.5.1",
    "i18next-fs-backend": "^2.2.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.2",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
