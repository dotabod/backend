{
  "name": "@dotabod/dota",
  "description": "By leveraging insights from the Dota 2 official API, Dotabod will know exactly when to hide sensitive streamer information or engage with your Twitch audience.",
  "version": "1.0.0",
  "license": "GPL-3.0-or-later",
  "author": "Geczy",
  "main": "src/index.ts",
  "exports": "./dist/index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "docker:production": "node --inspect=0.0.0.0:9229 ./dist/index.js",
    "docker:development": "tsx watch src/index.ts",
    "host:development": "doppler run --fallback-only -- bun docker:development",
    "build": "tsc --build --verbose",
    "debug": "nodemon -L --ext ts --ignore '__tests__/*' --exec 'node --inspect=0.0.0.0:9229 --trace-warnings -r ts-node/register --loader ts-node/esm src/index.ts'",
    "devscript": "doppler run -- node --no-warnings -r ts-node/register --loader ts-node/esm ./src/dev/index.ts",
    "fix-users-dev-docker": "node --no-warnings -r ts-node/register --loader ts-node/esm src/dev/index.ts"
  },
  "dependencies": {
    "@node-steam/id": "^1.2.0",
    "@supabase/supabase-js": "^2.42.5",
    "@twurple/api": "6.0.9",
    "@twurple/auth": "6.0.9",
    "@twurple/chat": "6.0.9",
    "@twurple/ebs-helper": "^7.0.7",
    "@twurple/eventsub-base": "6.0.9",
    "@twurple/eventsub-http": "6.0.9",
    "@types/long": "^5.0.0",
    "@types/lru-cache": "^7.10.10",
    "axios": "1.2.0-alpha.1",
    "axios-retry": "^3.9.1",
    "body-parser": "^1.20.2",
    "chokidar": "^3.6.0",
    "cors": "^2.8.5",
    "country-code-emoji": "^2.3.0",
    "dotaconstants": "^8.5.0",
    "express": "^4.19.2",
    "express-body-parser-error-handler": "^1.0.7",
    "i18next": "^23.11.2",
    "i18next-fs-backend": "^2.3.1",
    "lodash.isequal": "^4.5.0",
    "lru-cache": "^10.2.0",
    "mongodb": "^6.5.0",
    "redis": "^4.6.13",
    "retry": "^0.13.1",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "tsx": "^4.7.2",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@jest/globals": "^29.7.0",
    "@testing-library/dom": "^10.0.0",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/lodash.isequal": "^4.5.8",
    "@types/memoizee": "^0.4.11",
    "@types/node": "^20.12.7",
    "@types/retry": "^0.12.5",
    "@types/steam": "^0.0.32",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
