    "express": "^4.18.2",
    "dotaconstants": "^7.18.0",
    "@supabase/supabase-js": "^2.34.0",
    "mongodb": "^6.1.0",
    "lodash.isequal": "^4.5.0",
    "@dotabod/settings": "workspace:packages/settings",
    "axios": "1.2.0-alpha.1",
