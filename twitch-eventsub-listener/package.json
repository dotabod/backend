{
  "name": "dotabod-twitch-eventsub-server",
  "description": "We forward twitch events to dotabod.",
  "version": "1.0.0",
  "license": "GPL-3.0-or-later",
  "author": "Geczy",
  "main": "src/index.ts",
  "exports": "./dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node -r newrelic --trace-warnings ./src/index.js",
    "build": "tsc",
    "debug": "nodemon -L --ext ts --exec \"node --inspect=0.0.0.0:9229 -r ts-node/register --loader ts-node/esm src/index.ts\"",
    "dev": "nodemon -L --ext ts ./src/index.ts",
    "devnotdocker": "dotenv -e ../.env.local -- nodemon -L --ext ts ./src/index.ts",
    "postinstall": "prisma generate --schema ./prisma/postgres.prisma",
    "lint": "eslint --ext .ts ./src",
    "prettier": "prettier --ext .ts ./src --write",
    "lint:fix": "npm run lint -- --fix",
    "pullpsql": "dotenv -e ../.env.local -e ../.env.prod -- prisma db pull --schema ./prisma/postgres.prisma"
  },
  "dependencies": {
    "@prisma/client": "4.8.0",
    "@twurple/api": "^5.2.7",
    "@twurple/auth": "^5.2.7",
    "@twurple/eventsub-base": "^5.3.0-pre.1",
    "@twurple/eventsub-http": "^5.3.0-pre.1",
    "newrelic": "^9.7.4",
    "prisma": "^4.8.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.11",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.3"
  }
}
