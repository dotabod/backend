{
    "name": "dotabod-backend",
    "description": "By leveraging insights from the Dota 2 official API, Dotabod will know exactly when to hide sensitive streamer information or engage with your Twitch audience.",
    "version": "1.0.0",
    "license": "GPL-3.0-or-later",
    "author": "Geczy",
    "main": "src/index.ts",
    "exports": "./dist/index.js",
    "type": "module",
    "scripts": {
        "start": "node --trace-warnings ./src/index.js",
        "build": "tsc",
        "debug": "nodemon --ext ts --exec \"node --inspect=0.0.0.0:9229 -r ts-node/register -r dotenv/config --loader ts-node/esm src/index.ts\"",
        "dev": "nodemon -L --ext ts ./src/index.ts",
        "postinstall": "prisma generate",
        "lint": "eslint --ext .ts ./src",
        "lint:fix": "npm run lint -- --fix"
    },
    "dependencies": {
        "@node-steam/id": "^1.2.0",
        "@prisma/client": "^4.7.1",
        "@sentry/integrations": "^7.23.0",
        "@sentry/node": "^7.23.0",
        "@sentry/profiling-node": "^0.0.12",
        "@sentry/tracing": "^7.23.0",
        "@supabase/supabase-js": "^2.1.1",
        "@twurple/api": "^5.2.7",
        "@twurple/auth": "^5.2.7",
        "@twurple/chat": "^5.2.7",
        "@twurple/eventsub": "^5.2.7",
        "@types/long": "^5.0.0",
        "axios": "1.2.0-alpha.1",
        "axios-retry": "^3.3.1",
        "body-parser": "^1.20.1",
        "country-code-emoji": "^2.3.0",
        "dota2": "https://github.com/dotabod/node-dota2.git",
        "express": "^4.18.2",
        "git": "^0.1.5",
        "long": "5.0.1",
        "memoizee": "^0.4.15",
        "mongodb": "^4.12.1",
        "newrelic": "^9.7.3",
        "redis": "^4.5.1",
        "retry": "^0.13.1",
        "socket.io": "^4.5.4",
        "steam": "https://github.com/dotabod/node-steam",
        "steam-errors": "^1.0.0"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.2",
        "@types/express": "^4.17.14",
        "@types/memoizee": "^0.4.8",
        "@types/newrelic": "^9.4.0",
        "@types/node": "^18.11.11",
        "@types/retry": "^0.12.2",
        "@types/steam": "^0.0.29",
        "@typescript-eslint/eslint-plugin": "^5.45.1",
        "@typescript-eslint/parser": "^5.45.1",
        "dotenv": "^16.0.3",
        "eslint": "^8.29.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^3.5.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-unused-imports": "^2.0.0",
        "nodemon": "^2.0.20",
        "prettier": "^2.8.0",
        "prisma": "^4.7.1",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.1.1",
        "typescript": "^4.9.3"
    }
}
