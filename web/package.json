    "@twurple/chat": "^5.2.7",
    "@twurple/eventsub": "^5.2.7",
    "@types/long": "^5.0.0",
    "axios": "1.2.0-alpha.1",
    "axios-retry": "^3.3.1",
    "body-parser": "^1.20.1",
    "country-code-emoji": "^2.3.0",
    "dota2": "https://github.com/dotabod/node-dota2.git",
    "express": "^4.18.2",
    "git": "^0.1.5",
    "long": "5.0.1",
    "memoizee": "^0.4.15",
    "mongodb": "^4.12.1",
    "newrelic": "^9.7.4",
    "redis": "^4.5.1",
    "retry": "^0.13.1",
    "socket.io": "^4.5.4",
